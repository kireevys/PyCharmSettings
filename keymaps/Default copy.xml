<keymap version="1" name="Default copy" parent="$default">
  <action id="BLACKReformatCode">
    <keyboard-shortcut first-keystroke="meta alt l" />
    <keyboard-shortcut first-keystroke="windows" second-keystroke="alt l" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="alt f" />
    <keyboard-shortcut first-keystroke="ctrl alt l" />
  </action>
</keymap>